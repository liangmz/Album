<?xml version="1.0"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细信息，请访问
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <!--
    有关 .NET 4.5 的 web.config 更改的说明，请参见 http://go.microsoft.com/fwlink/?LinkId=235367。

    可在 <httpRuntime> 标记上设置以下特性。
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <httpRuntime useFullyQualifiedRedirectUrl="true" maxRequestLength="409600" executionTimeout="3000"/>
    <pages controlRenderingCompatibilityVersion="4.0">
      <namespaces>
        <add namespace="System.Net"/>
        <add namespace="System.Xml"/>
      </namespaces>
    </pages>
  </system.web>
  <appSettings>
  <add key="localhost.AblumService" value="http://localhost:86/AblumService.asmx"/>
  </appSettings>
  <system.webServer>
    <defaultDocument>
      <files>
        <add value="Default_Login_.aspx"/>
      </files>
    </defaultDocument>
  </system.webServer>
</configuration>